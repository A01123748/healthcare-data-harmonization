{
  "name": "whistle-language-server",
  "displayName": "Whistle Language Server",
  "description": "",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:plaintext"
  ],
  "main": "./out/client/src/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Whistle Language Server Configuration",
      "properties": {
        "whistleLspServerSettings.rootPath": {
          "type": "string",
          "url": "./",
          "default": "./"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b ./",
    "lint": "eslint ./server ./client --ext ts",
    "watch": "tsc -watch -b ./",
    "pretest": "npm run compile && npm run lint",
    "test": "ts-node ./node_modules/jasmine/bin/jasmine --config=./spec/support/jasmine.json"
  },
  "dev": "nodemon â€”exec babel-node completion.test.ts",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/glob": "^7.1.1",
    "@types/jasmine": "^3.5.14",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.11.0",
    "glob": "^7.1.6",
    "jasmine": "^3.6.1",
    "jasmine-spec-reporter": "^6.0.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.9.7",
    "vscode-languageclient": "^6.1.3",
    "vscode-languageserver": "^6.1.1",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "vscode-languageclient": "^6.1.3"
  }
}
